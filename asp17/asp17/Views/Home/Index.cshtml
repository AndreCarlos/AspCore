@model List<asp17.Models.Cliente>            

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h2>DADOS</h2>
    <hr />

    @*<p>Nome: @Model.nome</p>
    <p>Apelido: @Model.apelido</p>*@

    @foreach (var cliente in Model)
    {
        <p>Nome: @cliente.nome  @cliente.apelido - @Html.ActionLink("Detalhes", "detalhes", new {  id= cliente.id})</p>
    }
</body>
</html>
